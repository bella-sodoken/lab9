 void populateArray(int A[101][101], int N){
  int i,j;
  for(int c = 0; c<N;c++){
    i=rand()%101;
    j=rand()%101;
    if(A[i][j] ==-1)
      A[i][j] ==-1
    else
    A[i][j]=1;
    }
  }
  
  void searchValidEntries(int XL, int YL, int HX, int YH, int A[101][101]){
    cout<< "the positive outcome of the counts in the bounding box("<<XL<<","<<YL<<") and ("<<XH<<","<<YH<<") are " <<endl;
    for(int i XL;i<=<=XH;i++){
      for(int j YL;j<=<=YH;j++){
        if(A[i][j]!=-1{
        cout<<" "<<i<<j<<","<<A[i][j]<<endl;
        
 }
 }
 }
 cout <<endll

  
  
    
